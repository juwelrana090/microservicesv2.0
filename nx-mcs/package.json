    "@nx/js": "21.4.1",
    "@nx/nest": "^21.4.1",
    "@nx/node": "^21.4.1",
    "@nx/web": "21.4.1",
    "@nx/webpack": "21.4.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.38",
    "@types/jest": "^30.0.0",
    "@types/node": "20.19.9",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
